{
	"compileOnSave": true,
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Node",
		"target": "ES2020",
		"jsx": "react",
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"outDir": "src/generated",
		"sourceMap": true
	},
	"include": ["src/ts/*"],
	"exclude": ["node_modules", "**/node_modules/*"]
}
